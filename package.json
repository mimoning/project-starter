{
  "name": "dce-starter",
  "version": "0.1.0",
  "private": true,
  "main": "public/main.js",
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router": "^4.3.1"
  },
  "scripts": {
    "start": "concurrently 'npm run render' 'npm run main'",
    "build": "cross-env BROWSER=none react-scripts build",
    "render": "cross-env BROWSER=none react-scripts start",
    "main": "concurrently 'tsc -w ./public/main.ts' 'wait-on http://localhost:3000 && electron .'",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "render-in-browser": "react-scripts start"
  },
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "concurrently": "^4.0.1",
    "cross-env": "^5.2.0",
    "electron": "^3.0.6",
    "elemon": "^5.0.3",
    "react-scripts": "^2.1.1",
    "react-test-renderer": "^16.6.1",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6",
    "wait-on": "^3.1.0",
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/react-router": "^4.3.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "jest-enzyme": "^7.0.1",
    "node-sass": "^4.10.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
